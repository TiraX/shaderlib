<% include header.ejs %>


  <div class="container">
    <% include login_dlg.ejs %>
    <% include navbar.ejs %>	

    <blockquote>
      <p>
        <br></p>
    </blockquote>

    <div class="row clearfix">
      <div class="col-md-12 column">
        <div class="row">
        <% if (user) { %>
        <%   for(var m in user.upload_infos) { %>
        <%     var model = user.upload_infos[m]; %>
          <div class="col-md-3">
            <div class="thumbnail">
              <img alt="300x300" src="images/index_thumb1.jpg" />
              <div class="caption">
                <h3><%= model.name %></h3>
                <p>Time.</p>
                <p>
                  <a href="/edit?id=<%= model.id %>"><img src="/images/3d_setting.png" class="img-thumbnail" /></a></p>
              </div>
            </div>
          </div>
        <%   } %>
        <% } %>
        </div>
      </div>
    </div>
  </div>

<% include footer.ejs %>